package infytq;
import java.util.*;

public class infytq {
	 public static void main(String []args){
	        int[] input = new int[10];
	        for(int i = 0; i < 10; i++)
	            input[i] = i+1;
	        int temp = 0;
	        for(int left = 0 , right = input.length-1 ; left < right ; left++,right--) {
	        	temp = input[left];
	        	input[left] = input[right];
	        	input[right] = temp;
	        }
	        System.out.println(Arrays.toString(input));
	     }
}
