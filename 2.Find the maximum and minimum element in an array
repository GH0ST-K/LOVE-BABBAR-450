package infytq;
import java.util.*;
public class infytq {
	 public static void main(String []args){
	        int[] input = new int[10];
	        for(int i = 0; i < 10; i++)
	            input[i] = i+1;
	        Arrays.sort(input);
	        System.out.println("Maximum element in an array = "+input[input.length-1]+"\nSmallest element in an array = "+input[0]);
	     }
}
